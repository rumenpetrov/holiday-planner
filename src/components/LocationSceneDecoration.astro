<style>
  #decoration-pin {
    opacity: 0;
  }
</style>

<script>
  import { gsap } from "gsap";

  // document.addEventListener('astro:page-load', () => {
  const decorationPinElement = document.querySelector("#decoration-pin");
  const decorationTreeElements = document.querySelectorAll(".decoration-tree");

  const timeline = gsap.timeline();

  timeline.set([decorationPinElement, decorationTreeElements], {
    opacity: 1,
    transformPerspective: 300,
    perspective: 300,
    backfaceVisibility: "hidden",
    transformStyle: "preserve-3d",
    transformOrigin: "50% 100%",
  });

  timeline.from(decorationPinElement, {
    y: -50,
    delay: 0.3,
    duration: 0.5,
    ease: "bounce.out",
  });

  timeline.to([decorationPinElement, decorationTreeElements], {
    delay: 2,
    rotation: -6,
    duration: 1,
    stagger: 0.2,
  });

  timeline.to([decorationPinElement, decorationTreeElements], {
    rotation: 0,
    duration: 1,
    stagger: 0.2,
  });
  // });
</script>
